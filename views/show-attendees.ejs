<main class="contenedor asistentes">
    <h1><%= pageName%></h1>
    <% if (attendeesData.length > 0) { %>
        <ul class="listado-asistentes">
            <% attendeesData.forEach(attendee => { %>
                <li>
                    <div class="imagen">
                        <% if(attendee.image) { %>
                            <img src="/uploads/profiles/<%= attendee.image %>" alt="Profile Image">
                        <% } %>
                    </div>
                    <div class="texto">
                        <%= attendee.name%>
                    </div>
                </li>
            <% }) %>
        </ul>
    <% } else { %>
        <p class="no-asistentes">There are no attendees yet.</p>
    <% } %>
</main>